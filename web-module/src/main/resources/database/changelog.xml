    <changeSet id="44" author="portnova.julia@gmail.com">
        <createTable tableName="ORDER_STATUS">
            <column name="id" type="bigint" autoIncrement="true" incrementBy="1">
                <constraints primaryKey="true"/>
            </column>
            <column name="status_name" type="ENUM ('NEW', 'IN_PROGRESS', 'DELIVERED', 'REJECTED')">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>
    <changeSet id="45" author="portnova.julia@gmail.com">
        <createTable tableName="ORDERS">
            <column name="id" type="bigint" autoIncrement="true" incrementBy="1">
                <constraints primaryKey="true"/>
            </column>
            <column name="uuid" type="varchar(36)">
                <constraints nullable="false"/>
            </column>
            <column name="number" type="varchar(20)">
                <constraints nullable="false"/>
            </column>
            <column name="seller_id" type="bigint">
                <constraints foreignKeyName="fk_orders_sale_user_id" references="USER(id)"/>
            </column>
            <column name="seller" type="varchar(100)">
                <constraints nullable="false"/>
            </column>
            <column name="status_id" type="bigint">
                <constraints foreignKeyName="fk_orders_order_status_id" references="ORDER_STATUS(id)"/>
            </column>
            <column name="created_at" type="timestamp" defaultValueComputed="now()">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>
    <changeSet id="46" author="portnova.julia@gmail.com">
        <createTable tableName="ORDER_DETAIL">
            <column name="order_id" type="bigint">
                <constraints primaryKey="true" foreignKeyName="fk_orders_order_detail_id" references="ORDERS(id)"/>
            </column>
            <column name="item_name" type="varchar(100)">
                <constraints nullable="false"/>
            </column>
            <column name="total_price" type="DECIMAL(10,2)">
                <constraints nullable="false"/>
            </column>
            <column name="item_quantity" type="int">
                <constraints nullable="false"/>
            </column>
            <column name="customer_id" type="bigint">
                <constraints foreignKeyName="fk_order_detail_customer_user_id" references="USER(id)"/>
            </column>
            <column name="customer_telephone" type="varchar(20)">
                <constraints nullable="false"/>
            </column>
            <column name="customer_identifier" type="varchar(50)">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>
